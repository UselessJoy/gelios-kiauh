auto lo
ifcace lo inet loopback

auto eth0
iface eth0 inet static
    address 200.200.1.1
    netmask 255.255.255.0
    network 200.200.1.0
    broadcast 200.200.1.255

auto wlan0
    allow-hotplug wlan0
    iface wlan0 inet static
    address 20.20.1.1
    netmask 255.255.255.0
    network 20.20.1.0
    broadcast 20.20.1.255